-- Cria a se√ß√£o 'Toggles' na mainTab
local togglesSection = mainTab:CreateSection("Toggles")

-- Vari√°veis para o toggle
local noSit
local nositDied

-- Fun√ß√£o que desativa a a√ß√£o de sentar
local function noSitFunc()
    wait()
    local humanoid = Players.LocalPlayer.Character:FindFirstChildWhichIsA("Humanoid")
    if humanoid and humanoid.Sit then
        humanoid.Sit = false
    end
end

-- Adiciona o toggle para "N√£o se sentar em itens/carros" ap√≥s a se√ß√£o
local noSitToggle = mainTab:CreateToggle("N√£o se sentar em itens/carros", false, function(state)
    if state then
        -- Ativa a funcionalidade
        if noSit then noSit:Disconnect() end
        if nositDied then nositDied:Disconnect() end
        noSit = Players.LocalPlayer.Character:FindFirstChildOfClass('Humanoid'):GetPropertyChangedSignal("Sit"):Connect(noSitFunc)
        
        local function nositDiedFunc()
            repeat wait() until Players.LocalPlayer.Character ~= nil and Players.LocalPlayer.Character:FindFirstChildOfClass("Humanoid")
            noSit:Disconnect()
            noSit = Players.LocalPlayer.Character:FindFirstChildOfClass('Humanoid'):GetPropertyChangedSignal("Sit"):Connect(noSitFunc)
        end
        nositDied = Players.LocalPlayer.CharacterAdded:Connect(nositDiedFunc)
    else
        -- Desativa a funcionalidade
        if noSit then noSit:Disconnect() end
    end
end)

-- Posiciona o toggle na mainTab ap√≥s a se√ß√£o
mainTab:AddItem(noSitToggle, togglesSection)

